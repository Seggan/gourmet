@inline
fun print(c: Char) {
    asm[Nothing]("""
        print [c];
        flushStr;
    """);
}

@inline
fun print(i: Number) {
    asm[Nothing]("""
        print [i];
        flush;
    """);
}

@inline
fun read(): Number {
    let i: Number;
    asm[Nothing]("""
        read [i];
    """);
    return i;
}